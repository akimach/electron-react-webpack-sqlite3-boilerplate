{
  "name": "electron-skeleton",
  "version": "0.0.1",
  "description": "A Boilerplate of Electron for React/Webpack/SQLite3/ES6.",
  "main": "main.js",
  "author": "akimach",
  "license": "MIT",
  "repository": "https://github.com/akimach/electron-react-webpack-sqlite3-boilerplate.git",
  "homepage": "https://github.com/akimach/electron-react-webpack-sqlite3-boilerplate",
  "scripts": {
    "start": "./node_modules/.bin/webpack; ./node_modules/.bin/electron .",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "build"
  },
  "build": {
    "appId": "com.example.akimach",
    "files": [
      "main.js",
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "dependencies": {
    "react": "^16.1.1",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-builder": "^19.46.9",
    "electron-packager": "^10.1.0",
    "electron-rebuild": "^1.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  }
}
